{
  "manifest_version": 3,
  "name": "about:blank",
  "description": "Open \"about:blank\" when open a new tab.",
  "version": "0.0.1",
  "homepage_url": "https://github.com/fisker/about-blank",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'"
  }
}